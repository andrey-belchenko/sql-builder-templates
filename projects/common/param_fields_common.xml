<?xml version="1.0" encoding="utf-8"?>
<root>
  <fields>
    <field id="cmn_list" controlType="UIList" />
    <field id="cmn_list_yesno" controlType="UIList" type="number">
      <listquery>
        <query name="yn" />
      </listquery>
    </field>
    <field id="cmn_check" controlType="UICheck" checked="0" type="number" />
    <field id="cmn_date" controlType="UIDate" type="date" />
    <field id="cmn_string" controlType="UIText" type="string" />
    <field id="cmn_phone" controlType="UIText" type="string" edit-mask="s+0(000)000-0000"/>
    <field id="cmn_string_array" controlType="UITextArray" type="string" />
    <field id="cmn_text" controlType="UITextEx" type="string" />
    <field id="cmn_integer" controlType="UINumber" edit-mask="d" type="number" />
    <field id="cmn_integer_noincrement" controlType="UIText" edit-mask="N0" type="number" />
    <field id="cmn_float" controlType="UIText" edit-mask="N2" type="number" />
    <field id="cmn_currency" controlType="UINumber" edit-mask="c" type="number" />
    <field id="cmn_date_cur" controlType="UIDate" type="date">
      <defaultquery>
        <query name="today" />
      </defaultquery>
    </field>
    <field id="cmn_combo_str" controlType="UICombo" type="string" />
    <field id="cmn_combo_num" controlType="UICombo" type="number" />
  </fields>
</root>