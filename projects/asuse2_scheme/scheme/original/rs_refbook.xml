<?xml version="1.0" encoding="utf-8"?>
<root>
  <queries>
    <query name="rs_refbook">
      <select>
        <column table="a" column="kod_refbook" type="number" column-mandatory="1" />
        <column table="a" column="name" type="string" data-size="50" title="Наименование" comment="Наименование справочника" />
        <column table="a" column="pr_byt" type="number" title="" comment="признак быта: 0 - справочник только для юрлиц; 1 - для быта; NULL - для всех" />
        <column table="a" column="image" type="blob" title="" comment="Картинка" />
        <column table="a" column="dat_del" type="date" title="" comment="Дата передачи в архив" />
        <column table="a" column="u_m" type="string" data-size="30" title="" comment="Пользователь" />
        <column table="a" column="d_m" type="date" title="" comment="Дата последнего изменения" />
      </select>
      <from>
        <table name="rs_refbook" as="a" />
      </from>
    </query>
  </queries>
</root>