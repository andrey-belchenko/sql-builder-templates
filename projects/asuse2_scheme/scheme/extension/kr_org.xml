    <view name="kr_podr_byt">
      SELECT kodp, name, sname
      FROM kr_org a
      WHERE kod_ecls = 4
      AND kod_separator = 5
      AND kg_common.user_podr_rights (5, kodp)  &gt; 0
    </view>
    <view name="kr_org_ecls4">
      SELECT kodp
      FROM kr_org
      WHERE kod_ecls = 4
      AND kg_common.user_podr_rights (1, kodp)  &gt; 0
    </view>
  </views>
