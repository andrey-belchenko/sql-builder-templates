      <call function="over" as="ur_bk_kodp_last_st_name" group="max" type="string">
        <call function="last_value">
          <fact column="ur_stage_bk_st_name" agg="max" />
        </call>
        <call function="partition by">
          <column table="kod_folders" column="kodp" />
        </call>
