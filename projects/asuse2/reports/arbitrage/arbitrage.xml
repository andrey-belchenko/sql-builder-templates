<?xml version="1.0" encoding="utf-8"?>
<root>
  <queries>
    <query name="arbitrage-get_findoc_state">
      <params>
        <param name="kod_doc" type="number">
          <const>111691465</const>
        </param>
        <param name="deb_sf" type="number">
          <const>1</const>
        </param>
      </params>
      <select>
        <call function="case" type="string" as="state_name">
          <call function="when">
            <call function="=">
              <column table="a" column="state" />
              <const>6</const>
            </call>
            <const>' мораторная задолженность'</const>
          </call>
          <call function="when">
            <call function="in">
              <column table="a" column="state" />
              <const>(5,4,3)</const>
            </call>
            <const>' на исполнительном производстве'</const>
          </call>
          <call function="when">
            <call function="in">
              <column table="a" column="state" />
              <const>2</const>
            </call>
            <const>' взысканная задолженность'</const>
          </call>
          <call function="when">
            <call function="in">
              <column table="a" column="state" />
              <const>1</const>
            </call>
            <const>' исковая задолженность'</const>
          </call>
          <call function="else">
            <const>''</const>
          </call>
        </call>
      </select>
      <from>
        <query as="a">
          <select>
            <call function="case" as="state">
              <!--Банкротство - мараторная задолженность-->
              <call function="when">
                <call function="gt">
                  <query>
                    <select>
                      <column table="ur_dogplat" column="kod_dogplat" group="count" />
                    </select>
                    <from>
                      <query name="ur_dogplat">
                        <link name="kod_mat_ba">
                          <dlink name="ur_hist_mat" as="ur_hist_mat_ba" />
                        </link>
                        <link name="kod_deb">
                          <elink name="sr_facvip" />
                        </link>
                      </query>
                    </from>
                    <where>
                      <call function="and">
                        <call function="or">
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>0</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_deb" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>1</const>
                            </call>
                            <call function="or">
                              <call function="=">
                                <useparam name="kod_doc" />
                                <column table="ur_dogplat" column="kod_sf" />
                              </call>
                              <call function="=">
                                <useparam name="kod_doc" />
                                <column table="sr_facvip" column="kod_sf" />
                              </call>
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>2</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_izv" />
                            </call>
                          </call>
                        </call>
                        <call function="is not null">
                          <column table="ur_hist_mat_ba" column="dat_mor" />
                        </call>
                      </call>
                    </where>
                  </query>
                  <const>0</const>
                </call>
                <const>6</const>
              </call>
              <!--Инкассо-->
              <call function="when">
                <call function="gt">
                  <query>
                    <select>
                      <column table="ur_dogplat" column="kod_dogplat" group="count" />
                    </select>
                    <from>
                      <query name="ur_dogplat">
                        <link name="kod_mat_pp">
                          <dlink name="ur_hist_mat">
                            <dlink name="ur_isp">
                              <dlink name="ur_folders" as="ur_folders_ip">
                                <dlink name="ur_kazn" />
                              </dlink>
                            </dlink>
                          </dlink>
                        </link>
                      </query>
                    </from>
                    <where>
                      <call function="and">
                        <call function="or">
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>0</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_deb" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>1</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_sf" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>2</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_izv" />
                            </call>
                          </call>
                        </call>
                        <call function="is not null">
                          <column table="ur_kazn" column="dat_doc" />
                        </call>
                      </call>
                    </where>
                  </query>
                  <const>0</const>
                </call>
                <const>5</const>
              </call>
              <!--Казначейство-->
              <call function="when">
                <call function="gt">
                  <query>
                    <select>
                      <column table="ur_dogplat" column="kod_dogplat" group="count" />
                    </select>
                    <from>
                      <query name="ur_dogplat">
                        <link name="kod_mat_pp">
                          <dlink name="ur_hist_mat">
                            <dlink name="ur_isp">
                              <dlink name="ur_folders" as="ur_folders_ip">
                                <dlink name="ur_inkasso" />
                              </dlink>
                            </dlink>
                          </dlink>
                        </link>
                      </query>
                    </from>
                    <where>
                      <call function="and">
                        <call function="or">
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>0</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_deb" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>1</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_sf" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>2</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_izv" />
                            </call>
                          </call>
                        </call>
                        <call function="is not null">
                          <column table="ur_inkasso" column="dat_doc" />
                        </call>
                      </call>
                    </where>
                  </query>
                  <const>0</const>
                </call>
                <const>4</const>
              </call>
              <!--Исполнительное производство-->
              <call function="when">
                <call function="gt">
                  <query>
                    <select>
                      <column table="ur_dogplat" column="kod_dogplat" group="count" />
                    </select>
                    <from>
                      <query name="ur_dogplat">
                        <link name="kod_mat_pp">
                          <dlink name="ur_hist_mat">
                            <dlink name="ur_isp">
                              <dlink name="ur_folders" as="ur_folders_ip">
                                <dlink name="ur_mat" as="ur_mat_ip" />
                              </dlink>
                            </dlink>
                          </dlink>
                        </link>
                      </query>
                    </from>
                    <where>
                      <call function="and">
                        <call function="or">
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>0</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_deb" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>1</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_sf" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>2</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_izv" />
                            </call>
                          </call>
                        </call>
                        <call function="is not null">
                          <column table="ur_mat_ip" column="dat_doc" />
                        </call>
                      </call>
                    </where>
                  </query>
                  <const>0</const>
                </call>
                <const>3</const>
              </call>
              <!--Вступившее в силу решение-->
              <call function="when">
                <call function="gt">
                  <query>
                    <select>
                      <column table="ur_dogplat" column="kod_dogplat" group="count" />
                    </select>
                    <from>
                      <query name="ur_dogplat">
                        <link name="kod_mat_pp">
                          <dlink name="ur_hist_mat">
                            <link name="kod_result" />
                          </dlink>
                        </link>
                      </query>
                    </from>
                    <where>
                      <call function="and">
                        <call function="or">
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>0</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_deb" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>1</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_sf" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>2</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_izv" />
                            </call>
                          </call>
                        </call>
                        <call function="=">
                          <column table="kod_result" column="decision" />
                          <const>1</const>
                        </call>
                        <call function="lt">
                          <column table="ur_hist_mat" column="dat_entry" />
                          <const>sysdate</const>
                        </call>
                      </call>
                    </where>
                  </query>
                  <const>0</const>
                </call>
                <const>2</const>
              </call>
              <!--Иск-->
              <call function="when">
                <call function="gt">
                  <query>
                    <select>
                      <column table="ur_dogplat" column="kod_dogplat" group="count" />
                    </select>
                    <from>
                      <query name="ur_dogplat">
                        <link name="kod_mat_pp" />
                      </query>
                    </from>
                    <where>
                      <call function="and">
                        <call function="or">
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>0</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_deb" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>1</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_sf" />
                            </call>
                          </call>
                          <call function="and">
                            <call function="=">
                              <useparam name="deb_sf" />
                              <const>2</const>
                            </call>
                            <call function="=">
                              <useparam name="kod_doc" />
                              <column table="ur_dogplat" column="kod_izv" />
                            </call>
                          </call>
                        </call>
                        <call function="is not null">
                          <column table="kod_mat_pp" column="dat_doc" />
                        </call>
                      </call>
                    </where>
                  </query>
                  <const>0</const>
                </call>
                <const>1</const>
              </call>
              <call function="else">
                <const>0</const>
              </call>
            </call>
          </select>
          <from>
            <table name="dual" />
          </from>
        </query>
      </from>
    </query>
    <query name="word_print_test">
      <params>
        <param name="kod_mat" type="number">
          <const>1</const>
        </param>
      </params>
      <select>
        <column table="a" column="kod_mat_pp" group="1" />
        <column table="kod_mat_pp" column="num_reg" group="max" />
        <column table="kod_sud" column="name" as="sud_name" group="max" />
        <const as="date_cur" type="date">sysdate</const>
        <column table="a" column="summa_osn" group="sumnvl" />
      </select>
      <from>
        <query name="un-dogplat" as="a">
          <extendwhere>
            <call function="and">
              <call function="=">
                <column table="*" column="kod_abon_type" />
                <const name="kod_mat">1</const>
              </call>
              <call function="=">
                <column table="*" column="kod_mat_pp" />
                <useparam name="kod_mat" />
              </call>
            </call>
          </extendwhere>
          <link name="kod_mat_pp" join="inner">
            <link name="kod_sud" />
          </link>
        </query>
      </from>
      <where>
        <call function="and">
          <call function="true" />
        </call>
      </where>
    </query>
  </queries>
</root>