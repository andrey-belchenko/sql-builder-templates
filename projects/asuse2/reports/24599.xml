      </params>
      <select>
        <column table="dog" column="ndog" />
        <column table="dog" column="kod_dog" />
        <column table="sf_nach_ym_1" column="nachisl" as="nachisl_do_ymbeg" dgroup="sum" />
        <column table="sf_beg" column="ostatok" as="zadol_begin" dgroup="sum" />
