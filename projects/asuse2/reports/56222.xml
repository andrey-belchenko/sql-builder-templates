    <query name="56222_opl" timestamp="06.02.2022 00:47:02" materialize="1">
      <params>
        <param name="ym1">
          <const>2015.01</const>
        </param>
        <param name="ym2">
          <const>2015.01</const>
        </param>
        <param name="dep" />
