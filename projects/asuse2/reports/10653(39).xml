        </query>
      </from>
      <where>
        <call function="and">
          <call function="true" />
          <call function="between" optional="1">
            <column table="a" column="ym_akt_main" />
            <useparam name="ym1" />
            <useparam name="ym2" />
          </call>
        </call>
      </where>
    </query>
