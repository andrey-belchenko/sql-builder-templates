        end;
      </procedure>
      <print-templates>
        <excel>
          <template name="44047.xlsx" title="Справка инвентаризации по кредиторам" print-xlsx="1" print-proc="2" post-process="0" />
        </excel>
      </print-templates>
      <queries>
        <query name="44047" as="a" main="1" />
