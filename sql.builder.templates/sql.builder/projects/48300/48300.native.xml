    <query name="48300_period_ad_to_str" timestamp="23.10.2018 15:42:30" file="sql.builder.templates\sql.builder\projects\48300\reports\48300-rep.xml">
      <params>
        <param name="date_s" type="date">
          <const>date'2018-10-01'</const>
        </param>
        <param name="date_po" type="date">
          <const>date'2018-10-17'</const>
        </param>
      </params>
      <select>
        <call as="val" function="||">
          <call function="if">
            <call function="or">
              <call function="is not null">
                <useparam name="date_s" />
              </call>
              <call function="is not null">
                <useparam name="date_po" />
              </call>
            </call>
            <const>', дата этапа'</const>
          </call>
          <call function="if">
            <call function="is not null">
              <useparam name="date_s" />
            </call>
            <call function="||">
              <const>' с '</const>
              <call function="to_char">
                <useparam name="date_s" />
                <const>'DD.MM.YYYY'</const>
              </call>
            </call>
          </call>
          <call function="if">
            <call function="is not null">
              <useparam name="date_po" />
            </call>
            <call function="||">
              <const>' по '</const>
              <call function="to_char">
                <useparam name="date_po" />
                <const>'DD.MM.YYYY'</const>
              </call>
            </call>
          </call>
        </call>
      </select>
      <from>
        <query name="dual" as="a" />
      </from>
    </query>
    <query name="48300-is_viza_aux" timestamp="01.11.2018 14:51:29" title="Этапы по договору" file="sql.builder.templates\sql.builder\projects\48300\reports\48300-rep.xml">
      <select>
        <column table="b" column="kod_doc" group="1" />
        <column table="b" column="kod_spr_viz" group="count_dist" as="viz_count" />
      </select>
      <from>
        <query name="48300-is_viza" as="b" />
      </from>
      <where>
        <call function="and">
          <call function="in">
            <column table="b" column="kod_spr_viz" />
            <const>(141, 382)</const>
          </call>
          <call function="is not null">
            <column table="b" column="dat_viz" />
          </call>
        </call>
      </where>
    </query>
    <query name="48300-is_viza_interm" timestamp="01.11.2018 16:20:22" title="Этапы по договору" file="sql.builder.templates\sql.builder\projects\48300\reports\48300-rep.xml">
      <params>
        <usepart part="48300-params" />
      </params>
      <select>
        <column table="b" column="kod_u" />
        <column table="b" column="kod_doc" />
        <column table="b" column="kod_spr_viz" />
        <column table="b" column="dat_post" />
        <column table="b" column="dat_viz" />
        <column table="b" column="kod_person" />
        <call as="row_num" function="row_number">
          <call function="partition by">
            <column table="b" column="kod_doc" />
            <call function="case">
              <call function="when">
                <call function="in">
                  <column table="b" column="kod_spr_viz" />
                  <const>(37, 141, 382, 385, 671)</const>
                </call>
                <const>1</const>
              </call>
              <call function="when">
                <call function="in">
                  <column table="b" column="kod_spr_viz" />
                  <const>(13, 125, 670, 485)</const>
                </call>
                <const>2</const>
              </call>
            </call>
          </call>
          <call function="order by 2">
            <call function="desc nulls last">
              <column table="b" column="dat_viz" />
            </call>
            <call function="if" exclude="1">
              <call function="and">
                <call function="=">
                  <call function="nvl0">
                    <column table="kod_doc" column="viz_count" />
                  </call>
                  <const>2</const>
                </call>
                <call function="=">
                  <column table="b" column="kod_spr_viz" />
                  <const>141</const>
                </call>
              </call>
              <call function="null" />
              <column table="b" column="dat_viz" />
            </call>
          </call>
        </call>
      </select>
      <from>
        <query name="48300-is_viza" as="b" />
        <query name="48300-is_viza_aux" join="left outer" as="kod_doc">
          <call function="=">
            <column table="b" column="kod_doc" />
            <column table="kod_doc" column="kod_doc" />
          </call>
        </query>
      </from>
      <where>
        <call function="and">
          <call function="ge" optional="1">
            <call function="trunc">
              <column table="b" column="dat_viz" />
            </call>
            <call function="trunc">
              <useparam name="date_s" />
            </call>
          </call>
          <call function="le" optional="1">
            <call function="trunc">
              <column table="b" column="dat_viz" />
            </call>
            <call function="trunc">
              <useparam name="date_po" />
            </call>
          </call>
          <call function="or">
            <call function="and">
              <call function="=">
                <column table="kod_doc" column="viz_count" />
                <const>2</const>
              </call>
              <call function="!=">
                <column table="b" column="kod_spr_viz" />
                <const>141</const>
              </call>
            </call>
            <call function="!=">
              <call function="nvl0">
                <column table="kod_doc" column="viz_count" />
              </call>
              <const>2</const>
            </call>
          </call>
        </call>
      </where>
    </query>
