      <from>
        <query as="u">
          <union>
            <query>
              <select>
                <const type="number" as="kvartal">1</const>
                <const type="string" as="name">'1 квартал'</const>
              </select>
              <from>
                <table name="dual" />
              </from>
            </query>
            <query>
              <select>
                <const>2</const>
                <const>'2 квартал'</const>
