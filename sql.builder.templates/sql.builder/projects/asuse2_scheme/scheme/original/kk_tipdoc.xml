<?xml version="1.0" encoding="utf-8"?>
<root xmlns="sqlbuilder">
  <queries>
    <query name="kk_tipdoc" class="1" title="Тип документа">
      <select>
        <column table="a" column="kod_tipdoc" type="number" />
        <column table="a" column="name" type="string" title="Тип документа" vid="1" />
        <column table="a" column="abbr" type="string" title="Аббревиатура" />
        <column table="a" column="srok" type="number" title="Срок действия мес." editor="spin" />
        <column table="a" column="ur_prizn" type="number" title="Вид док-та" />
        <column table="a" column="prioritet" type="number" title="Приоритет" editor="spin" />
        <column table="a" column="doc_one" type="number" title="0- много док на дог,1-только один" />
        <column table="a" column="template" type="string" title="путь и имя шаблона документа" />
        <column table="a" column="pr_val" type="number" title="признак привязки расчета [ тепло2 ]" />
        <column table="a" column="prizn_ur" type="number" comment="1 - документ относится только к юл&#xA;2 - документ относится к договору&#xA;0,null - годится для всего" />
      </select>
      <from>
        <table name="kk_tipdoc" as="a" />
      </from>
    </query>
  </queries>
</root>