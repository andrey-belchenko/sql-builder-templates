<root>
  <queries>
    <query name="ur_pretenz">
      <select>
        <column table="a" column="kod_pretenz" type="number"></column>
        <column table="a" column="kod_folders" type="number"></column>
        <column table="a" column="kod_subject" type="number"></column>
        <column table="a" column="kod_state" type="number"></column>
        <column table="a" column="num_reg" type="string"></column>
        <column table="a" column="num_otv" type="string"></column>
        <column table="a" column="dat_do" type="date"></column>
        <column table="a" column="dat_otv" type="date"></column>
        <column table="a" column="reply" type="string"></column>
        <column table="a" column="prim" type="string"></column>
        <column table="a" column="u_m" type="string"></column>
        <column table="a" column="d_m" type="date"></column>
        <column table="a" column="dat_doc" type="date"></column>
        <column table="a" column="dat_srok" type="date"></column>
        <column table="a" column="kod_vid_pretenz" type="number" title="вид претензии"></column>
        <column table="a" column="proc" type="number" title="проценты за польз ден. средствами, вводимые юристами ( ст. 395 гк рф)"></column>
        <column table="a" column="kodp" type="number"></column>
        <column table="a" column="ogr" type="number" title="расходы за введение ограничения режима электропотребления"></column>
        <column table="a" column="tep_el" type="number" title="1-электр,2-тепло"></column>
        <column table="a" column="type_potr" type="number" title="1-юр,2-физ"></column>
        <column table="a" column="vvod" type="number" title="0-из бд,1-вручную ввод суммы"></column>
        <column table="a" column="kod_emp" type="number" title="код сотрудника"></column>
        <column table="a" column="kod_vi" type="number" title="1- исходящие, 2- входящие"></column>
        <column table="a" column="dep" type="number" title="код подразделения"></column>
        <column table="a" column="dat_finish" type="date"></column>
      </select>
      <from>
        <table name="ur_pretenz" as="a"></table>
        <query name="us_subject" as="kod_subject" join="left outer">
          <call function="=">
            <column table="a" column="kod_subject"></column>
            <column table="kod_subject" column="kod_subject"></column>
          </call>
        </query>
        <query name="us_state" as="kod_state" join="left outer">
          <call function="=">
            <column table="a" column="kod_state"></column>
            <column table="kod_state" column="kod_state"></column>
          </call>
        </query>
        <query name="ur_folders" as="kod_folders" join="left outer">
          <call function="=">
            <column table="a" column="kod_folders"></column>
            <column table="kod_folders" column="kod_folders"></column>
          </call>
        </query>
        <query name="us_vid_pretenz" as="kod_vid_pretenz" join="left outer">
          <call function="=">
            <column table="a" column="kod_vid_pretenz"></column>
            <column table="kod_vid_pretenz" column="kod_vid_pretenz"></column>
          </call>
        </query>
      </from>
    </query>
  </queries>
</root>