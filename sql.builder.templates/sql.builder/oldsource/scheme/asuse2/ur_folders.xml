<root>
  <queries>
    <query name="ur_folders">
      <select>
        <column table="a" column="kod_folders" type="number"></column>
        <column table="a" column="kodp" type="number"></column>
        <column table="a" column="kod_emp" type="number" title="код сотрудника"></column>
        <column table="a" column="kod_vi" type="number"></column>
        <column table="a" column="kod_sdp" type="number"></column>
        <column table="a" column="dat_create" type="date"></column>
        <column table="a" column="zadol" type="number"></column>
        <column table="a" column="u_m" type="string"></column>
        <column table="a" column="d_m" type="date"></column>
        <column table="a" column="kod_parent" type="number"></column>
        <column table="a" column="kod_isp" type="number"></column>
        <column table="a" column="tep_el" type="number" title="1-электр,2-тепло"></column>
        <column table="a" column="vvod" type="number" title="0-из бд,1-вручную ввод суммы"></column>
        <column table="a" column="dat_finish" type="date" title="дата сброса папки в архив"></column>
        <column table="a" column="num_doc" type="string" title="док-т - основание для архивации папки"></column>
        <column table="a" column="kod_podr" type="number" title="код подразделения"></column>
        <column table="a" column="tep_el_byt" type="number" title="1-электр,2-тепло"></column>
        <column table="a" column="kodp_new" type="number" title="замена ответчика - новый код"></column>
        <column table="a" column="kodps" type="string" title="код абонента строка"></column>
        <column table="a" column="namep" type="string" title="имя абонета"></column>
        <column table="a" column="kod_category" type="number" title="категория абонента"></column>
        <column table="kodp" column="nump"></column>
      </select>
      <from>
        <table name="ur_folders" as="a"></table>
        <query name="uk_sdp" as="kod_sdp" join="left outer">
          <call function="=">
            <column table="a" column="kod_sdp"></column>
            <column table="kod_sdp" column="kod_sdp"></column>
          </call>
        </query>
        <query name="uk_vi" as="kod_vi" join="left outer">
          <call function="=">
            <column table="a" column="kod_vi"></column>
            <column table="kod_vi" column="kod_vi"></column>
          </call>
        </query>
        <query name="ur_folders" as="kod_parent" join="left outer">
          <call function="=">
            <column table="a" column="kod_parent"></column>
            <column table="kod_folders" column="kod_folders"></column>
          </call>
        </query>
        <query name="ur_isp" as="kod_isp" join="left outer">
          <call function="=">
            <column table="a" column="kod_isp"></column>
            <column table="kod_isp" column="kod_isp"></column>
          </call>
        </query>
        <query name="kr_employee" as="kod_emp" join="left outer">
          <call function="=">
            <column table="a" column="kod_emp"></column>
            <column table="kod_emp" column="kod_emp"></column>
          </call>
        </query>
        <query name="kr_org" as="kod_podr" join="left outer">
          <call function="=">
            <column table="a" column="kod_podr"></column>
            <column table="kodp" column="kodp"></column>
          </call>
        </query>
        <query name="kr_payer" as="kodp_new" join="left outer">
          <call function="=">
            <column table="a" column="kodp_new"></column>
            <column table="kodp" column="kodp"></column>
          </call>
        </query>
        <query name="kr_payer" as="kodp" join="left outer">
          <call function="=">
            <column table="a" column="kodp"></column>
            <column table="kodp" column="kodp"></column>
          </call>
        </query>
      </from>
    </query>
  </queries>
</root>