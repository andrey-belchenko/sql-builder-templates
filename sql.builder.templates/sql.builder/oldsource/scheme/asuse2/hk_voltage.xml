<root>
  <queries>
    <query name="hk_voltage">
      <select>
        <column table="a" column="voltage" type="number">
        </column>
        <column table="a" column="kod_sort" type="number">
        </column>
        <column table="a" column="abbr" type="string" title="Уровень напряжения">
        </column>
      </select>
      <from>
        <table name="hk_voltage" as="a">
        </table>
      </from>
    </query>
    <query name="hk_voltage_null">
      <select>
        <const>null</const>
        <const>null</const>
      </select>
      <from>
        <table name="dual">
        </table>
      </from>
    </query>
    <query name="hk_voltage+null">
      <union>
        <query name="hk_voltage">
        </query>
        <query name="hk_voltage_null">
        </query>
      </union>
    </query>
  </queries>
</root>